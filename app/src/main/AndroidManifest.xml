<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.apahayo.tubespabpaim">

    <application
        android:name=".Home"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".LoginActivity"
            android:screenOrientation="portrait"></activity>
        <activity android:name=".NavBotActivity"
            android:screenOrientation="portrait"/>
        <activity android:name=".KuisionerActivity"
            android:screenOrientation="portrait"/>
        <activity android:name=".HasilActivity"
            android:screenOrientation="portrait"/>
        <activity android:name=".MainActivity"
            android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".WelcomeActivity"
            android:screenOrientation="portrait"/>
        <activity android:name=".PetunjukActivity"
            android:screenOrientation="portrait"/>
        <activity android:name=".DetailAktifitas"
            android:screenOrientation="portrait"/>
        <activity android:name=".DaftarActivity"
            android:screenOrientation="portrait"/>

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>